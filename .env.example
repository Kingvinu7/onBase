# OnchainKit API Key - Get from https://portal.cdp.coinbase.com/
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_api_key_here

# Etherscan API Key (Now supports Base + other L2s) - Get from https://etherscan.io/apis
NEXT_PUBLIC_ETHERSCAN_API_KEY=your_etherscan_api_key_here

# OR Basescan API Key (Base-specific) - Get from https://basescan.org/apis  
NEXT_PUBLIC_BASESCAN_API_KEY=your_basescan_api_key_here

# Optional: Custom deployment URL
NEXT_PUBLIC_URL=http://localhost:3000